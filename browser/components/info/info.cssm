#info {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    flex: 1;
    overflow-x: hidden;
    background-color: var(--color-bg-info);
    position: relative;
}

#content-wrapper {
    padding: 5px 60px 10px 20px;
    width: 100%;
    overflow-y: scroll;
    justify-content: center;
    flex: 1;
}
